{
  "env": {
    "browser": true
  },
  "extends": ["plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "camelcase": "off",
    "no-unused-vars": "off"
  },
  "settings": {
    "react": {
      "version": "17"
    }
  }
}
