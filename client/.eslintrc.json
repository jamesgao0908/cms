{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google", "plugin:react/recommended", "plugin:prettier/recommended"],
  
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "camelcase": "off"
  },
  "settings": {
    "react": {
      "version": "17"
    }
  }
}
